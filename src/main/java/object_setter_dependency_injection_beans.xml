<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student"
		class="studyonline.object_dependency_injection.Student">
		<property name="studentId" value="10001"></property>
		<property name="activities">
			<bean class="studyonline.object_dependency_injection.Activities"></bean>
		</property>
		<property name="friends">
			<bean class="studyonline.object_dependency_injection.Friends"></bean>
		</property>
	</bean>

</beans>